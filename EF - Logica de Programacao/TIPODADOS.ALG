Algoritmo "TipoDados"

Var
   nome: caractere
   idade: inteiro
   altura: real
   crianca: logico

Inicio
   nome <- "Ingrid"
   idade <- 29
   altura <- 1.55
   crianca <- falso

   escreval("Nome: ", nome)
   escreval("Idade: ", idade)
   escreval("Altura: ", altura)
   escreval("É criança? ", crianca)


Fimalgoritmo